<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/maelstrom/clipboardAction.xsd">
	<import>
		<action name="enable">
			<actionclassname>news\system\clipboard\action\NewsClipboardAction</actionclassname>
			<showorder>1</showorder>
			<pages>
				<page>news\page\NewsOverviewPage</page>
			</pages>
		</action>
		<action name="disable">
			<actionclassname>news\system\clipboard\action\NewsClipboardAction</actionclassname>
			<showorder>2</showorder>
			<pages>
				<page>news\page\NewsOverviewPage</page>
			</pages>
		</action>
		<action name="trash">
			<actionclassname>news\system\clipboard\action\NewsClipboardAction</actionclassname>
			<showorder>3</showorder>
			<pages>
				<page>news\page\NewsOverviewPage</page>
			</pages>
		</action>
		<action name="restore">
			<actionclassname>news\system\clipboard\action\NewsClipboardAction</actionclassname>
			<showorder>4</showorder>
			<pages>
				<page>news\page\NewsOverviewPage</page>
			</pages>
		</action>
		<action name="delete">
			<actionclassname>news\system\clipboard\action\NewsClipboardAction</actionclassname>
			<showorder>5</showorder>
			<pages>
				<page>news\page\NewsOverviewPage</page>
			</pages>
		</action>
	</import>
</data>
